{
    "name": "electronjs",
    "version": "1.0.0",
    "description": "my awesome electron-app",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "electron-builder"
    },
    "electronDownload": {
        "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "build": {
        "appId": "com.baoxiaohe.aiPlugins",
        "productName": "ai-plugins",
        "win": {
            "icon": "build/favicon.ico",
            "target": [
                "nsis"
            ]
        },
        "mac": {
            "icon": "build/favicon.ico",
            "target": [
                "dmg"
            ],
            "extraResources":["zw/hello.html"],
            "hardenedRuntime": false
        },
        "nsis": {
            "oneClick": false,
            "perMachine": true,
            "allowToChangeInstallationDirectory": true
        }
    },
    "dependencies": {
        "fs-extra": "^10.1.0"
    },
    "devDependencies": {
        "electron": "^19.0.3",
        "electron-builder": "^23.0.3"
    },
    "author": "timibadass",
    "license": "ISC"
}
